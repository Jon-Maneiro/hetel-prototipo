  m_TableData:
  - m_Id: 12211777536
    m_Localized: ZIBERSEGURTASUNA
    m_Metadata:
      m_Items: []
  - m_Id: 14749331456
    m_Localized: Zibersegurtasunaren arloan, sistemak, sareak eta datuak zibermehatxuen
      aurka babestea da ardatz nagusia. Arlo honetako profesionalek ahuleziak identifikatzen,
      segurtasun-irtenbideak inplementatzen, gorabeherei erantzuten eta erabiltzaileak
      lineako praktika seguruen inguruan hezten lan egiten dute.
    m_Metadata:
      m_Items: []
  - m_Id: 17702121472
    m_Localized: 'Ahultasunen analisia eta ebaluazioa:

      Sistema informatikoetan
      eta sareetan izan daitezkeen segurtasun-hausteak identifikatzen eta ebaluatzen
      dituzte, baita arriskuak arintzeko eta defentsak sendotzeko estrategiak garatzen
      ere.'
    m_Metadata:
      m_Items: []
  - m_Id: 20021571584
    m_Localized: "Segurtasun-irtenbideen ezarpena eta mantentze-lanak: \nsegurtasun-softwarea
      eta hardwarea instalatzen, konfiguratzen eta mantentzen dute, hala nola suebakiak,
      intrusioak detektatzeko sistemak eta enkriptatze-tresnak, aktibo digitalak
      zibermehatxuetatik babesteko."
    m_Metadata:
      m_Items: []
  - m_Id: 22303272960
    m_Localized: 'Segurtasun-gorabeherei erantzuna:

      Azkar eta eraginkortasunez
      jarduten dute segurtasun-hausteak gertatzen direnean eduki eta konpontzeko,
      gertakariaren jatorria ikertuz eta etorkizuneko erasoak saihesteko neurri zuzentzaileak
      aplikatuz.'
    m_Metadata:
      m_Items: []
  - m_Id: 25155399680
    m_Localized: 'Segurtasunerako hezkuntza eta sentsibilizazioa:

      Langileei
      eta erabiltzaileei prestakuntza eta sentsibilizazioa eskaintzen diete lineako
      segurtasun-jardunbide onei buruz, ziber-mehatxuei buruz sentsibilizatuz eta
      beren burua behar bezala babesten irakatsi.'
    m_Metadata:
      m_Items: []
