  m_TableData:
  - m_Id: 10194317312
    m_Localized: 'Informazioaren Teknologiak (IT):

      Big Data eta IoT irtenbideak
      garatzeko, inplementatzeko eta mantentzeko enpresetan lan egin.'
    m_Metadata:
      m_Items: []
  - m_Id: 13906276352
    m_Localized: "Osasuna: \nAplikatu Big Data eta IoT teknologiak pazienteen datuen
      kudeaketa, osasunaren jarraipena eta gailu mediko adimendunen garapena hobetzeko."
    m_Metadata:
      m_Items: []
  - m_Id: 17093947392
    m_Localized: 'Energia:

      Energiaren sektoreari laguntzea, datuak erabiliz
      energiaren sorkuntzan eta banaketan eraginkortasuna optimizatzeko, baita azpiegituren
      monitorizaziorako IoT irtenbideak ezarriz.'
    m_Metadata:
      m_Items: []
  - m_Id: 26023620608
    m_Localized: 'Fabrikazioa:

      Inplementatu IoT sistemak fabrikazio-prozesuak
      optimizatzeko eta datuen analisiak erabiltzeko ekoizpen-kalitatea eta eraginkortasuna
      hobetzeko.'
    m_Metadata:
      m_Items: []
  - m_Id: 809616076800
    m_Localized: 'Finantza:

      Aplikatu datuen analitika finantza-erabakiak hartzeko
      eta garatu IoT irtenbideak aktiboen kudeaketarako eta finantza-segurtasunerako.'
    m_Metadata:
      m_Items: []
  - m_Id: 815588765696
    m_Localized: 'Telekomunikazioak:

      Lagun ezazu sare adimendunen garapenean
      eta kudeatu konektatutako gailuek sortutako datu kopuru handiak.'
    m_Metadata:
      m_Items: []
  - m_Id: 1167109189632
    m_Localized: 'Hezkuntza:

      Aplikatu datuen analitika irakaskuntza-metodoak
      hobetzeko eta erabili IoT gailuak ikaskuntza-ingurune adimentsuak sortzeko.'
    m_Metadata:
      m_Items: []
  - m_Id: 1169659326464
    m_Localized: 'Garraioa eta logistika:

      Inplementatu Big Data soluzioak
      garraio-bideak optimizatzeko eta flotaren jarraipena eta kudeaketarako IoT
      gailuak erabiltzeko.'
    m_Metadata:
      m_Items: []
  - m_Id: 1606806466560
    m_Localized: 'Gobernu:

      Sektore publikoari laguntzea datuak erabiliz erabakiak
      hartzeko eta gobernuaren zerbitzuen eraginkortasuna hobetzeko.'
    m_Metadata:
      m_Items: []
  - m_Id: 1838164275200
    m_Localized: 'Komunikabideak eta entretenimendua:

      Erabili datuak erabiltzailearen
      esperientzia pertsonalizatzeko entretenimendu-plataformetan eta aprobetxatu
      IoT esperientzia interaktiboak sortzeko.'
    m_Metadata:
      m_Items: []
  - m_Id: 1978786705408
    m_Localized: 'Ikerketa zientifikoa:

      Besteak beste, astronomia, biologia,
      ingurumena, besteak beste, datu multzo handiak aztertzea eskatzen duten ikerketa-proiektuetan
      lankidetzan aritzea.'
    m_Metadata:
      m_Items: []
  - m_Id: 735218614988800
    m_Localized: 'Arlo honetako hainbat rolekin lotutako zeregin komunen artean honako
      hauek aurki ditzakegu:'
    m_Metadata:
      m_Items: []
